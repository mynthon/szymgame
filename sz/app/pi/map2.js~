Sz_App_Pi_Boo = Sz_Engine_Quark_Static.extend({
	onAdd: function(){
		this.availSectors = []
		this.walkable = []
		this.sectors = []
		this.width = 4
		this.height = 1
	},


	addSectors: functon(){
		this.availSectors.push(new Sz_App_Pi_Sectors_1000100110001100a())
		this.availSectors.push(new Sz_App_Pi_Sectors_1000110010001001a())
	},

	checkArea: function(initx, inity){
		var ret = ['n', 'e', 's', 'w']
		var emptyEdge = 1000
		
		//check n
		if (inity === 0) {
			ret[0] = emptyEdge
		) else if(
			this.sectors[inity-1] === undefined ||
			this.sectors[inity-1][initx] === undefined
		) {
			ret[0] = -1
		} else {
			ret[0] = this.sectors[inity-1][initx].calcSExits()
		}
		
		//check e (unfinished)
		if (initx === this.width-1) {
			ret[3] = emptyEdge
		) else if(
			this.sectors[inity+1] === undefined ||
			this.sectors[inity+1][initx] === undefined
		) {
			ret[3] = -1
		} else {
			ret[3] = this.sectors[inity+1][initx].calcNExits()
		}

		//check s
		if (inity === this.height - 1) {
			ret[2] = emptyEdge
		) else if(
			this.sectors[inity+1] === undefined ||
			this.sectors[inity+1][initx] === undefined
		) {
			ret[2] = -1
		} else {
			ret[2] = this.sectors[inity+1][initx].calcNExits()
		}


		
	}

})
